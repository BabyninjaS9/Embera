{
    "name": "mpratt/embera",
    "type": "library",
    "description": "Oembed consumer library. Converts urls into their html embed code. Supports 60+ sites, such as Youtube, Twitter, vimeo, etc.",
    "keywords": ["Embed", "Oembed", "Auto embed", "Embed Text", "Url Embed", "Youtube",  "Vimeo", "Vine", "Twitter", "Instagram"],
    "homepage": "https://github.com/mpratt/Embera",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Pratt",
            "email": "yo@michael-pratt.com",
            "homepage": "http://www.michael-pratt.com",
            "role": "Author/Developer"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {"Embera\\": "src/Embera"}
    },
    "autoload-dev": {
        "psr-4": {"Embera\\": "tests/Embera"}
    }
}
